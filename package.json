    "@rollup/plugin-typescript": "^11.1.5",
    "@types/node": "^20.10.1",
    "chai": "^4.3.10",
    "mocha": "^10.2.0",
    "puppeteer": "^21.5.2",
    "rimraf": "^5.0.5",
